@model IList<AdminChavpWeb.Models.MethodDetailModel>

@{
    ViewBag.Title = "Method Perf";
}

<ul data-role="listview" data-inset="true" id="myList">
    <li data-role="list-divider">: @ViewBag.MachineName . @ViewBag.ServiceName . @ViewBag.MethodName</li>
    @foreach (var methodDetail in Model)
    {
        <li><a href='@Url.Action("MethodDetails", new { machineName = ViewBag.MachineName, serviceName = ViewBag.ServiceName, methodName = ViewBag.MethodName, callMMddyyyy = methodDetail.Date.ToString("MM.dd.yyyy") })')'>@methodDetail.Date.ToString("MM/dd/yyyy") <p>: @methodDetail.AverageElapsedMilliseconds.ToString("#.0#") ms/times </p><span class="ui-li-count">@methodDetail.TotalUsage</span></a></li>
    }
</ul>

<div data-role="footer" class="ui-bar">
	<a href='@Url.Action("Services")' data-role="button" data-icon="arrow-l">Services</a>
</div>